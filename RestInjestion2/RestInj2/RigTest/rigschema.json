{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "id",
    "rig_location",
    "well_bore_diameter",
    "well_name",
    "well_location",
    "well_diameter",
    "well_depth",
    "raw_data_file",
    "notes",
    "created_at",
    "modified_at"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "rig_location": {
      "$id": "#/properties/rig_location",
      "type": "string",
      "title": "The Rig_location Schema",
      "default": "",
      "examples": [
        "Houston"
      ],
      "pattern": "^(.*)$"
    },
    "well_bore_diameter": {
      "$id": "#/properties/well_bore_diameter",
      "type": "string",
      "title": "The Well_bore_diameter Schema",
      "default": "",
      "examples": [
        "100"
      ],
      "pattern": "^(.*)$"
    },
    "well_name": {
      "$id": "#/properties/well_name",
      "type": "string",
      "title": "The Well_name Schema",
      "default": "",
      "examples": [
        "Well1"
      ],
      "pattern": "^(.*)$"
    },
    "well_location": {
      "$id": "#/properties/well_location",
      "type": "string",
      "title": "The Well_location Schema",
      "default": "",
      "examples": [
        "Houston"
      ],
      "pattern": "^(.*)$"
    },
    "well_diameter": {
      "$id": "#/properties/well_diameter",
      "type": "integer",
      "title": "The Well_diameter Schema",
      "default": 0,
      "examples": [
        100
      ]
    },
    "well_depth": {
      "$id": "#/properties/well_depth",
      "type": "integer",
      "title": "The Well_depth Schema",
      "default": 0,
      "examples": [
        90
      ]
    },
    "raw_data_file": {
      "$id": "#/properties/raw_data_file",
      "type": "string",
      "title": "The Raw_data_file Schema",
      "default": "",
      "examples": [
        "c:\\temp\\raw.csv"
      ],
      "pattern": "^(.*)$"
    },
    "notes": {
      "$id": "#/properties/notes",
      "type": "string",
      "title": "The Notes Schema",
      "default": "",
      "examples": [
        "test data"
      ],
      "pattern": "^(.*)$"
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The Created_at Schema",
      "default": "",
      "examples": [
        "2019-10-08T20:22:22.501453Z"
      ],
      "pattern": "^(.*)$"
    },
    "modified_at": {
      "$id": "#/properties/modified_at",
      "type": "string",
      "title": "The Modified_at Schema",
      "default": "",
      "examples": [
        "2019-10-08T20:22:41.288019Z"
      ],
      "pattern": "^(.*)$"
    }
  }
}